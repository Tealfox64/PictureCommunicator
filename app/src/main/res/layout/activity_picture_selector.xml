<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cs246.picturecommunicator.PictureSelector">

    <GridLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <ImageButton
            android:id="@+id/medicalButton"
            android:layout_width="185dp"
            android:layout_height="246dp"
            android:layout_column="6"
            android:layout_row="0"
            app:srcCompat="@drawable/healthicon125" />

        <ImageButton
            android:id="@+id/foodButton"
            android:layout_width="185dp"
            android:layout_height="246dp"
            android:layout_column="5"
            android:layout_row="1"
            app:srcCompat="@drawable/foodicon125" />

        <ImageButton
            android:id="@+id/familyButton"
            android:layout_width="185dp"
            android:layout_height="246dp"
            android:layout_column="5"
            android:layout_row="0"
            app:srcCompat="@drawable/familyicon125" />

        <ImageButton
            android:id="@+id/activitiesButton"
            android:layout_width="185dp"
            android:layout_height="246dp"
            android:layout_column="6"
            android:layout_row="1"
            app:srcCompat="@drawable/activitiesicon125" />

    </GridLayout>
</android.support.constraint.ConstraintLayout>
